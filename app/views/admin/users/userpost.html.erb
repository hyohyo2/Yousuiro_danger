<div class="row">
  <%= render 'layouts/sidebar'%>
  <div class="col-10">
    <h3 class="row my-4">
      <div class="col-1"></div>
      <span class='heading-title'><%= @user.name %>さんの投稿一覧</span>
    </h3>
    <div class="row my-3 mr-4">
      <%= render "admin/posts/index", posts: @posts %>
    </div>
    <div class="d-flex justify-content-center">
      <%= paginate @posts, theme: 'bootstrap-5'%>
    </div>
  </div>
</div>